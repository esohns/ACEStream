<script type="text/javascript">
    function yl_get_pricerange(yield_id) {
      if (typeof yield_id === "undefined" || yield_id == '' || typeof yl === "undefined" || yl.YpResult === "undefined") return;
      var yp_pos = yield_id.split(',');
      for (var index = 0; index < yp_pos.length; index++) {
        var yp_res = yl.YpResult.get(yp_pos[index]);
        if (typeof yp_res !== "undefined" && yp_res.pricerange) {
          return yp_res.pricerange;
        }
      }
    }

